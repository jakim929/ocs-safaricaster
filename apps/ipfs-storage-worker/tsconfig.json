{
	"compilerOptions": {
		"target": "ESNext",
		"module": "ESNext",
		"moduleResolution": "Bundler",
		"strict": true,
		"skipLibCheck": true,

		"baseUrl": ".",
		"paths": {
			"@/*": ["./src/*"]
		},

		"lib": ["ESNext"],
		"types": ["@cloudflare/workers-types"],
		"jsx": "react-jsx",
		"jsxImportSource": "hono/jsx",

		"outDir": "types"
	}
}
